(begin
    (define x 512)
    (define y 512)
    (define out (tex x y))
    (define out2 (tex x y))
    (define out3 (tex x y))
    (fill out (col 0.5 0 0))
    (fill out2 (col 0.5 0.1 0.3))
    (fill out3 (col 0 0 0))
    (linear-gradient out2 (vec2 0 0) (vec2 1 1) 1)
    (radial-gradient out (vec2 0 0) 1.0)
    (radial-gradient out3 (vec2 0 1) 2.5)
    (rotate-scale out 2 (vec2 5 6))
    (distort out out3 out2 2.5)
    (add-scale out out2 (col 0.2 0.1 0.1) (col 0.1 0.2 0.3))
    (radial-gradient out2 (vec2 0 0) 5.0)
    (add-scale out out2 (col -1.5 1.1 1.1) (col 0.1 0.2 0.3))
   )
